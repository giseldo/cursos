  isbn            = {9781538604571},
  journal         = {Proceedings - 30th IEEE Conference on Computer Vision and Pattern Recognition, CVPR 2017},
  keywords        = {A1,artigo,chatbot},
  mendeley-groups = {chatbot - Artigos,chatbot - Todos,chatbot - A1,chatbot - Ainda n{\~{a}}o referenciado},
  mendeley-tags   = {A1,artigo,chatbot},
  number          = {5},
  pages           = {1080--1089},
  title           = {{Visual dialog}},
  volume          = {2017-Janua},
  year            = {2017}
}
