@phdthesis{Lima2017,
  abstract        = {Chatterbots s{\~{a}}o aplica{\c{c}}{\~{o}}es que t{\^{e}}m como objetivo simular uma conversa real com um ser humano, de forma com que eles se comportem tamb{\'{e}}m como humanos. A principal ideia em quest{\~{a}}o {\'{e}} fazer como que as duas partes do di{\'{a}}logo conversem sobre um determinado dom{\'{i}}nio de conhecimento, de forma que a conversa gire inteligentemente em torno deste dom{\'{i}}nio. A partir de um di{\'{a}}logo s{\~{a}}o emitidas diversas informa{\c{c}}{\~{o}}es que podem, e devem ser relevantes para o dom{\'{i}}nio em quest{\~{a}}o. Assim, essas informa{\c{c}}{\~{o}}es consequentemente geram conhecimento e aprendizado por meio das partes envolvidas no di{\'{a}}logo. Isso {\'{e}} bastante comum nas conversas de chat, fazendo com que sejam bastante utilizadas como fonte de conhecimento. De acordo com Thomas Gruber (1993), ontologia {\'{e}} uma especifica{\c{c}}{\~{a}}o expl{\'{i}}cita de uma conceitualiza{\c{c}}{\~{a}}o. {\'{E}} um recurso de grande utilidade para representa{\c{c}}{\~{a}}o de conhecimento. A ontologia encontra-se em n{\'{i}}vel de abstra{\c{c}}{\~{a}}o t{\~{a}}o alto, que a mesma estabelece uma terminologia comum e n{\~{a}}o amb{\'{i}}gua para o dom{\'{i}}nio em quest{\~{a}}o. Isso faz com que ela seja um recurso fortemente adequado para seu uso como modelo de dados, uma vez que a representa{\c{c}}{\~{a}}o dos conceitos do dom{\'{i}}nio proposto “ensina” o chatterbot, possibilitando a realiza{\c{c}}{\~{a}}o de infer{\^{e}}ncias sobre os objetos, e o torna apto para conversar com o usu{\'{a}}rio de maneira mais natural poss{\'{i}}vel. Existe um grande desafio no que diz respeito a extra{\c{c}}{\~{a}}o de conhecimento a partir de linguagem na natural, uma vez que h{\'{a}} uma variabilidade na forma como as pessoas escrevem e falam. Isso dificulta a obten{\c{c}}{\~{a}}o de conhecimento por meio dos chatterbots a partir de linguagem natural. O objetivo geral deste projeto {\'{e}} demostrar que a constru{\c{c}}{\~{a}}o e representa{\c{c}}{\~{a}}o do conhecimento decorrentes de di{\'{a}}logos entre pessoas e chatterbots {\'{e}} uma solu{\c{c}}{\~{a}}o vi{\'{a}}vel para o processo de aquisi{\c{c}}{\~{a}}o de um modelo de dom{\'{i}}nio baseado em ontologias, modelado em l{\'{o}}gica de descri{\c{c}}{\~{a}}o.},
  annote          = {Flavia Andre
                     
                     Parei aqui.},
  author          = {Lima, Carlos Eduardo Teixeira Lima},
  file            = {:C\:/Mendeley/Lima/Unknown/Lima - 2017 - Um Chatterbot para Criacao e Desenvolvimento de Ontologias com Logica de Descricao.pdf:pdf},
  keywords        = {UFPE,aprender,chatbot,disserta{\c{c}}{\~{a}}o},
  mendeley-groups = {chatbot - UFPE,chatbot - Todos},
  mendeley-tags   = {UFPE,aprender,chatbot,disserta{\c{c}}{\~{a}}o},
  pages           = {102},
  title           = {{Um Chatterbot para Criacao e Desenvolvimento de Ontologias com Logica de Descricao}},
  year            = {2017}
}
