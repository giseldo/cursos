@article{Jenkins2011,
  abstract        = {Chatbot sistemas orientados a servicios se utilizan para informar a los usuarios de una manera coloquial acerca de un determinado servicio o producto en un sitio web. Nuestra investigaci{\'{o}}n muestra que los sistemas actuales son mucho tiempo para construir y no muy precisa o satisfactoria para los usuarios. Nos encontramos con que la comprensi{\'{o}}n del lenguaje natural y m{\'{e}}todos de generaci{\'{o}}n de lenguaje natural son fundamentales para la creaci{\'{o}}n de un sistema eficiente y {\'{u}}til. En esta tesis se ha investigado m{\'{e}}todos actuales y pasados ​​en esta {\'{a}}rea de investigaci{\'{o}}n y hacer especial hincapi{\'{e}} en Gram{\'{a}}tica de la Construcci{\'{o}}n y su implementaci{\'{o}}n computacional. Nuestra investigaci{\'{o}}n muestra que los usuarios tienen fuertes reacciones emotivas a c{\'{o}}mo se comportan estos sistemas, por lo que tambi{\'{e}}n investigue el componente de interacci{\'{o}}n persona-ordenador. Presentamos tres sistemas (KIA, John y KIA2), y llevamos a cabo pruebas de usuario extensos sobre todos ellos, as{\'{i}} como pruebas comparativas. KIA se construye utilizando m{\'{e}}todos exisitng, John se construye con el usuario en mente y KIA2 se construye utilizando el m{\'{e}}todo de construcci{\'{o}}n gramatical. Se encontr{\'{o}} que el enfoque de construcci{\'{o}}n de la gram{\'{a}}tica se desempe{\~{n}}a bien en sistemas orientados al servicio chatbots, y que los usuarios lo prefiero sobre otros sistemas.},
  author          = {Jenkins, Marie-Claire},
  file            = {:C\:/Mendeley/Jenkins/Unknown/Jenkins - 2011 - Designing Service-Oriented Chatbot Systems Using a Construction Grammar-Driven Natural Language Generation System.pdf:pdf},
  mendeley-groups = {chatbot - Todos,chatbot - Ainda n{\~{a}}o referenciado},
