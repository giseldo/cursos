  isbn            = {9788578110796},
  issn            = {1098-6596},
  journal         = {Journal of Chemical Information and Modeling},
  keywords        = {UFPE,chatbot,dissertacao,disserta{\c{c}}ao,icle},
  mendeley-groups = {chatbot - Disserta{\c{c}}{\~{o}}es,chatbot - UFPE,chatbot - Todos},
  mendeley-tags   = {UFPE,chatbot,dissertacao,disserta{\c{c}}ao},
