@phdthesis{Nakabayashi2009,
  abstract        = {A presente pesquisa tem como tema central investigar o conceito de Intelig{\^{e}}ncia e, especificamente, de Intelig{\^{e}}ncia Artificial (IA), derivado das Ci{\^{e}}ncias da Computa{\c{c}}{\~{a}}o e sua repercuss{\~{a}}o nas pesquisas da Filosofia da Mente. Procura conceituar e entender tal perspectiva {\`{a}} luz da Tecnoci{\^{e}}ncia e da Cibern{\'{e}}tica, tendo em vista os conceitos que buscam compreender a mente humana pela imita{\c{c}}{\~{a}}o de seu comportamento, aplicando os conceitos {\`{a}} investiga{\c{c}}{\~{a}}o e simula{\c{c}}{\~{a}}o de di{\'{a}}logos: os chamados chatterbots. A pesquisa parte de um estudo metodol{\'{o}}gico e bibliogr{\'{a}}fico diversificado dos conceitos de Intelig{\^{e}}ncia Artificial (IA) e das concep{\c{c}}{\~{o}}es de Filosofia da Mente, enfocando a proemin{\^{e}}ncia do tema em seus aspectos interdisciplinares. Discute as perspectivas consolidadas na comunidade acerca do tema, especialmente as abordagens de Searle (1984, 1992, 1997), Gardner (1994, 1995) e, no panorama brasileiro, de Teixeira (1990, 1994, 1995, 1998, 2000, 2004, 2008), incidindo nas rela{\c{c}}{\~{o}}es entre os conceitos de intencionalidade, c{\'{e}}rebro, experimento mental e cognitivismo, bem como algumas tend{\^{e}}ncias cr{\'{i}}ticas e de contesta{\c{c}}{\~{a}}o de seus limites formais. Valendo-se de pesquisas anteriores sobre o tema e de metodologia que mescla elementos computacionais e de filosofia da mente, analisa o experimento conceitual do quarto chin{\^{e}}s de Searle (em seus aspectos de sintaxe e sem{\^{a}}ntica). A partir deste experimento e seus resultados, apresenta a perspectiva da investiga{\c{c}}{\~{a}}o da Intelig{\^{e}}ncia Artificial (IA), como pertencente {\`{a}} natureza da cogni{\c{c}}{\~{a}}o, apoiada na teoria da cogni{\c{c}}{\~{a}}o, nomeadamente de fen{\^{o}}menos como categoriza{\c{c}}{\~{a}}o e identifica{\c{c}}{\~{a}}o de objetos, resolu{\c{c}}{\~{a}}o de problemas, decis{\~{a}}o e consci{\^{e}}ncia. A pesquisa culmina na an{\'{a}}lise do conceito de Intelig{\^{e}}ncia Artificial (IA), na Filosofia da Mente, proposto como um elemento noem{\'{a}}tico, constituindo-se como condi{\c{c}}{\~{a}}o para a produ{\c{c}}{\~{a}}o do conhecimento cient{\'{i}}fico, aplicado este nas {\'{a}}reas de Tecnologia da Intelig{\^{e}}ncia e Design Digital. Aplica os conceitos e resultados alcan{\c{c}}ados {\`{a}} defini{\c{c}}{\~{a}}o e extens{\~{a}}o do conceito de hipertexto, tomando-o como um mecanismo computacional capaz de estruturar di{\'{a}}logos hierarquizados, indexados, ao modo da formula{\c{c}}{\~{a}}o de Robot de Primo (2001) e Roth Coelho (2001). Por fim, os pressupostos investigados s{\~{a}}o apresentados em resultados observados nos chamados chatterbots na {\'{a}}rea de Intelig{\^{e}}ncia Artificial (IA), destacando suas caracter{\'{i}}sticas e sua import{\^{a}}ncia no atual contexto de sua utiliza{\c{c}}{\~{a}}o computacional no ciberespa{\c{c}}o. Palavras},
  annote          = {Artigo que fala sobre {\'{e}}tica e IA},
  author          = {Nakabayashi, Luciana Akemi},
  file            = {:C\:/Mendeley/Nakabayashi/Unknown/Nakabayashi - 2009 - A contribuicao da Inteligencia Artificial na Filosofia da Mente.pdf:pdf},
  keywords        = {chatbot,etica},
  mendeley-groups = {chatbot - Todos,chatbot - etica,chatbot - pscologia,chatbot - Ainda n{\~{a}}o referenciado},
  mendeley-tags   = {chatbot,etica},
  pages           = {109},
  title           = {{A contribuicao da Inteligencia Artificial na Filosofia da Mente}},
  year            = {2009}
}
