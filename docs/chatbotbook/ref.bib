@phdthesis{NevesAndre2005,
  abstract        = {O trabalho de pesquisa aqui apresentado teve como objetivo principal melhorar o desempenho de chatterbots em di{\'{a}}logos livres com usu{\'{a}}rios. Chatterbots s{\~{a}}o sistemas computacionais que se prop{\~{o}}em a conversar em linguagem natural como se fossem humanos. O primeiro desses sistemas foi ELIZA, desenvolvido em 1965 por Weizenbaum. Desde ent{\~{a}}o, in{\'{u}}meros sistemas foram produzidos com esse mesmo objetivo. Por{\'{e}}m, uma s{\'{e}}rie de problemas ainda continuam em aberto, dentre os quais, o tratamento de inten{\c{c}}{\~{a}}o, quest{\~{a}}o central na interpreta{\c{c}}{\~{a}}o de di{\'{a}}logos naturais. Nesse sentido, desenvolvemos um mecanismo para tratamento de inten{\c{c}}{\~{a}}o para ser incorporado a chatterbots baseados em AIML. Adotamos como base conceitual para o trabalho a Teoria da An{\'{a}}lise da Conversa{\c{c}}{\~{a}}o (TAC), por considerar a inten{\c{c}}{\~{a}}o em pares adjacentes, e n{\~{a}}o apenas na senten{\c{c}}a do falante, como a Teoria dos Atos de Fala. Com base na TAC e em experimentos realizados, selecionamos um conjunto de inten{\c{c}}{\~{o}}es, que foram utilizadas na cria{\c{c}}{\~{a}}o de regras em AIML que utilizam informa{\c{c}}{\~{o}}es de intencionalidade para interpretar e gerar senten{\c{c}}as em di{\'{a}}logos naturais. A solu{\c{c}}{\~{a}}o final foi testada em uma s{\'{e}}rie de experimentos, e demonstrou ser capaz de corrigir alguns problemas presentes em di{\'{a}}logos com chatterbots. Por exemplo, o sistema baseado em AIML padr{\~{a}}o tratou 40% das senten{\c{c}}as dos usu{\'{a}}rios como sendo desconhecidas, enquanto o nosso sistema classificou apenas 3,5% das senten{\c{c}}as como totalmente desconhecidas. Al{\'{e}}m disso, o sistema foi capaz de manter a estrutura global dos di{\'{a}}logos, criticando turnos de abertura ou fechamento que foram ditos no desenvolvimento, ou turnos de desenvolvimento ditos na abertura ou fechamento. Por fim, implementamos tr{\^{e}}s aplica{\c{c}}{\~{o}}es com chatterbots, o que demonstra que a solu{\c{c}}{\~{a}}o adotada favorece o reuso de categorias em bases AIML, processo extremamente custoso do ponto de vista de engenharia de software com os sistemas atuais.},
  author          = {{Neves, Andre}, M.},
  file            = {:C\:/Mendeley/Neves, Andre/Unknown/Neves, Andre - 2005 - iAML Um mecanismo para o tratamento de intencao em Chatterbots.pdf:pdf},
  keywords        = {UFPE,chatbot,disserta{\c{c}}{\~{a}}o},
  mendeley-groups = {chatbot - UFPE,chatbot - Todos,chatbot - Disserta{\c{c}}{\~{o}}es},
  mendeley-tags   = {UFPE,chatbot,disserta{\c{c}}{\~{a}}o},
  title           = {{iAML: Um mecanismo para o tratamento de intencao em Chatterbots}},
  year            = {2005}
}
