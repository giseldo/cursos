  isbn            = {9781595937995},
  issn            = {00978418},
  journal         = {SIGCSE'08 - Proceedings of the 39th ACM Technical Symposium on Computer Science Education},
  keywords        = {Accessibility,Blind students,Chatbots},
  mendeley-groups = {chatbot - Todos},
  pages           = {449--453},
  title           = {{Inspiring blind high school students to pursue computer science with instant messaging chatbots}},
