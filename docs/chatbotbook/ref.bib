  pages           = {74},
  title           = {{Chatterbot para esclarecimento de duvidas dos clientes de uma loja de departamentos}},
  year            = {2009}
}
@phdthesis{Diegues2013,
  abstract        = {A presente pesquisa prop{\~{o}}e um estudo acerca das varia{\c{c}}{\~{o}}es ling{\"{u}}{\'{i}}sticas presentes no {\^{a}}mbito das linguagens especializadas. Especificamente, realizou-se o estudo das varia{\c{c}}{\~{o}}es denominativas, entendidas como formas lingu{\'{i}}sticas diferentes cujo significado {\'{e}} equivalente, em CLEs (Combinat{\'{o}}rias Lexicais Especializadas), isto {\'{e}}, express{\~{o}}es lingu{\'{i}}sticas polilexicais, formadas por um n{\'{u}}cleo terminol{\'{o}}gico e respectivos coocorrentes. Procedeu-se {\`{a}} classifica{\c{c}}{\~{a}}o e considera{\c{c}}{\~{a}}o de todos os tipos de varia{\c{c}}{\~{o}}es encontradas para tais combinat{\'{o}}rias, a saber: gr{\'{a}}fica, morfossint{\'{a}}tica, lexical e redu{\c{c}}{\~{a}}o. O foco de an{\'{a}}lise, no entanto, esteve calcado na varia{\c{c}}{\~{a}}o morfossint{\'{a}}tica. O objetivo da an{\'{a}}lise foi a busca de elementos contextuais que estivessem condicionando ou indicando a presen{\c{c}}a da varia{\c{c}}{\~{a}}o morfossint{\'{a}}tica. A partir da detec{\c{c}}{\~{a}}o das causas da varia{\c{c}}{\~{a}}o e posterior busca por elementos formais que refletissem tais causas, realizou-se a sistematiza{\c{c}}{\~{a}}o de tais elementos, com o intuito de fornecer subs{\'{i}}dios para o estabelecimento de regras informatizadas que pudessem auxiliar na cria{\c{c}}{\~{a}}o futura de um programa de extra{\c{c}}{\~{a}}o autom{\'{a}}tica de varia{\c{c}}{\~{o}}es. A an{\'{a}}lise foi efetuada a partir de quatro corpora pertencentes {\`{a}}s {\'{a}}reas de Cardiologia, Gest{\~{a}}o Ambiental, Enfermagem e Inform{\'{a}}tica que pudessem estar equiparados quanto {\`{a}} situa{\c{c}}{\~{a}}o comunicativa e contribuir para a sistematiza{\c{c}}{\~{a}}o da varia{\c{c}}{\~{a}}o especializada em diferentes {\^{a}}mbitos do conhecimento. Toda a pesquisa foi orientada pelas correntes te{\'{o}}ricas atuais da Terminologia Comunicativa, que direcionam o olhar para a caracteriza{\c{c}}{\~{a}}o das diferentes situa{\c{c}}{\~{o}}es em que dada comunica{\c{c}}{\~{a}}o se estabelece para levar a cabo o estudo de termos e CLEs. A partir dos dados coletados e de sua an{\'{a}}lise, verificou-se que uma sistematiza{\c{c}}{\~{a}}o da varia{\c{c}}{\~{a}}o morfossint{\'{a}}tica terminol{\'{o}}gica {\'{e}} poss{\'{i}}vel. Encontraram-se elementos formais presentes no contexto imediato em que as CLEs ocorreram que poder{\~{a}}o servir de ind{\'{i}}cios para a detec{\c{c}}{\~{a}}o automatizada de CLEs},
  author          = {Diegues, Cleo de Souza},
  file            = {:C\:/Mendeley/Diegues/Unknown/Diegues - 2013 - A variacao morfossintatica na terminologia.pdf:pdf},
  keywords        = {chatbot,disserta{\c{c}}{\~{a}}o},
  mendeley-groups = {chatbot - Disserta{\c{c}}{\~{o}}es,chatbot - Todos,chatbot - Ainda n{\~{a}}o referenciado},
  mendeley-tags   = {chatbot,disserta{\c{c}}{\~{a}}o},
  pages           = {146},
  school          = {UNIVERSIDADE FEDERAL DO RIO GRANDE DO SUL},
  title           = {{A variacao morfossintatica na terminologia}},
  year            = {2013}
}
@phdthesis{Pivetta2016,
  abstract        = {Esta tese prop{\~{o}}e um framework para intera{\c{c}}{\~{a}}o em ambiente virtual de ensino e aprendizagem bil{\'{i}}ngue (l{\'{i}}ngua portuguesa e l{\'{i}}ngua de sinais) fundamentada na Teoria da Cogni{\c{c}}{\~{a}}o Situada em Comunidades de Pr{\'{a}}tica. O processo te{\'{o}}rico orientador requer que os ambientes virtuais contemplem os aspectos da socializa{\c{c}}{\~{a}}o, participa{\c{c}}{\~{a}}o, engajamento, relacionamento, aprendizagem e compartilhamento, visando {\`{a}} cria{\c{c}}{\~{a}}o de valores para o capital do conhecimento. O Ambiente virtual de ensino e aprendizagem (AVEA) Moodle, por ser colaborativo, visual, de c{\'{o}}digo fonte aberto, pass{\'{i}}vel de integrar tecnologias internas e externas e usado em grande escala na educa{\c{c}}{\~{a}}o (presencial e {\`{a}} dist{\^{a}}ncia), foi selecionado como objeto de estudo. Por outro lado, a literatura e as pesquisas realizadas nesta tese mostram que o Moodle carece de aprimoramento para atender {\`{a}}s diferentes habilidades. O procedimento metodol{\'{o}}gico contemplou pesquisas de cunho qualitativo e explorat{\'{o}}rio, por meio de entrevistas, ensaios de intera{\c{c}}{\~{a}}o e question{\'{a}}rios, a fim de avaliar o Moodle com o p{\'{u}}blico alvo e elicitar requisitos para propor um prot{\'{o}}tipo de ambiente virtual bil{\'{i}}ngue. A otimiza{\c{c}}{\~{a}}o do Moodle foi executada por meio do desenvolvimento e integra{\c{c}}{\~{a}}o de novas tecnologias, gerando o prot{\'{o}}tipo que foi denominado de MooBi (Moodle Bil{\'{i}}ngue). Para a especifica{\c{c}}{\~{a}}o dos requisitos, foi realizada uma verifica{\c{c}}{\~{a}}o do MooBi com o p{\'{u}}blico alvo, utilizando as t{\'{e}}cnicas ensaios de intera{\c{c}}{\~{a}}o e question{\'{a}}rio. A an{\'{a}}lise e verifica{\c{c}}{\~{a}}o dos requisitos permitiram detectar inconformidades, gerar especifica{\c{c}}{\~{o}}es, sugest{\~{o}}es e novas recomenda{\c{c}}{\~{o}}es, dando origem ao framework Interato, uma estrutura conceitual para um AVEA bil{\'{i}}ngue com cria{\c{c}}{\~{a}}o de valores em Comunidades de Pr{\'{a}}tica.},
