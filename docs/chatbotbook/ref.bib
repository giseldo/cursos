  keywords        = {Chatbot,Metadata,Natural language,Ontologies,Semantic similarity,Semantic web},
  mendeley-groups = {chatbot - Todos,chatbot - Ainda n{\~{a}}o referenciado},
  number          = {5},
  pages           = {168--176},
  title           = {{A chatbot for automatic processing of learner concerns in an online learning platform}},
  volume          = {9},
  year            = {2018}
}
@article{BrunoMarietto2013,
